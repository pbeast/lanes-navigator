{
    "name": "lanes-navigator",
    "displayName": "Lanes navigator",
    "description": "Allows to navigate lanes defined in a Fastfile",
    "version": "0.0.1",
    "publisher": "PBeast",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onLanguage:ruby"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ],
        "languages": [
            {
                "id": "ruby",
                "extensions": [ "" ],
                "aliases": [ "Fastfile" ]
            }
        ]
    },
    "capabilities" : {
        "definitionProvider" : "true"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
